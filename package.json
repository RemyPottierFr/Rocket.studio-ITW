{
  "name": "svelte-challenge-rocket-studio",
  "description": "Svelte challenge for Rocket Studio interview",
  "version": "0.0.1",
  "author": "RÃ©my Pottier",
  "license": "MIT",
  "scripts": {
    "watch:tailwind": "TAILWIND=true postcss src/styles/tailwind.css -o static/global.css -w",
    "sapper": "sapper dev",
    "dev": "run-p --race watch:tailwind sapper",
    "lint:prettier": "prettier --check '**/*.{js,ts,svelte,html,json,md,css}'",
    "format:prettier": "prettier --write '**/*.{js,ts,svelte,html,json,md,css}'",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "compression": "^1.7.1",
    "cssnano": "^4.1.10",
    "husky": "^4.2.5",
    "polka": "next",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "sirv": "^0.4.0",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:prettier && node ./husky.hook.js",
      "pre-push": "yarn lint:prettier && node ./husky.hook.js"
    }
  }
}
